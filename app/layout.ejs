            <span class="navbar-toggler-icon"></span>
          </button>
        </div>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav align-items-center ms-auto mb-2 mb-lg-0 ">
            <!-- 手機會消失 -->
            <li class="nav-item d-lg-flex d-none align-items-center border-start border-dark py-8 px-12 ">
              <input type="text" class="bg-secondary border-0 me-6" id="search" placeholder="搜尋作品名稱、藝術家名稱">
              <label for="search" class="form-label mb-0 nav-link p-0" style="cursor: pointer;">
                <i class="fa-solid fa-magnifying-glass fs-5"></i>
              </label>
            </li>

            <li class="nav-item border-start-lg border-bottom border-dark py-lg-8 px-lg-12 py-6 text-center " style="margin-bottom: -4px ;">
              <a class="nav-link fw-bold p-0 fs-lg-8 fs-7" href="probe.html">探索</a>
            </li>
